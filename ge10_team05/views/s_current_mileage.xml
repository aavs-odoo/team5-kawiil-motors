<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ge10_team05.website_snippet" model="ir.asset">
        <field name="name">Website Snippet</field>
        <field name="bundle">web.assets_frontend</field>
        <field name="path">ge10_team05/static/src/js/javascript.js</field>
    </record>

    <template id="registry_snippet_id" inherit_id="website.snippets" name="Mileage Counter">
        <xpath expr="//div[@id='snippet_effect']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="ge10_team05.snippet_registry"/>
        </xpath>
    </template>

    <template id="snippet_registry" name="Mileage Counter Section">
        <section>
            <div class="container">
                <div class="row">
                    <div class="col text-center">
                        <div id="products">
                            Aqui apareceran productos
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </template>
</odoo>
